<%- include("partials/header"); -%>
<h1>Compose</h1>

<div class="container">
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label for="title">Title</label>
      <input class="form-control" type="text" name="postTitle">
    </div>
    <div class="form-group">
      <label>Post</label>
      <textarea class="form-control" name="postBody" rows="8" cols="30"></textarea>
    </div>
    <button class="btn btn-info" type="submit" name="button">Publish</button>
  </form>
</div>


<%- include("partials/footer"); -%>
